.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
} 
.flex{
  display: flex;
  box-sizing: border-box;
  justify-content: space-between;
  align-content: space-between;
  align-items: center;
}
.flexCenter{
  display: flex;
  box-sizing: border-box;
  justify-content: center;
  align-content: center;
  align-items: center;
}
.xuanxiang{
  padding:20rpx 50rpx ;
  font-size: 12pt;
  background: #ddd;
}
