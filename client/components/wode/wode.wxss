.wode {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding:30rpx;
  border-bottom: #ddd 1rpx solid;
}

.wode1 {
  display: flex;
  align-items: center;
}

.wode2 {
  overflow: hidden;
}

.wode3 {
  width: 100%;
  height: 100%;
}

.wode4, .wode5, .wode6 {
  width: 400rpx;
  overflow: hidden;
  flex-wrap: wrap;
  align-items: center;
  align-content: center;
  line-height: 40rpx;
}

.wode4 {
  margin-left: 30rpx;
  display: flex;
  width: 300rpx;
}

.wode5 {
  font-family: '方正兰亭纤黑_GBK Regular', '方正兰亭纤黑_GBK';
  font-weight: 400;
  font-style: normal;
  font-size: 36rpx;
}

.wode6 {
  font-family: 'Arial Normal', 'Arial';
  font-weight: 400;
  font-style: normal;
  font-size: 20rpx;
  color: #999;
}

.wode7 {
  color: #999;
}
